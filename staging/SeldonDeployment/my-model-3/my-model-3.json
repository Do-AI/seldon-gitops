{
    "kind": "SeldonDeployment",
    "apiVersion": "machinelearning.seldon.io/v1",
    "metadata": {
        "name": "my-model-3",
        "namespace": "staging",
        "creationTimestamp": null
    },
    "spec": {
        "name": "my-model",
        "predictors": [
            {
                "name": "main",
                "graph": {
                    "name": "model",
                    "type": "MODEL"
                },
                "componentSpecs": [
                    {
                        "metadata": {
                            "creationTimestamp": null
                        },
                        "spec": {
                            "containers": [
                                {
                                    "name": "model",
                                    "image": "seldonio/fixed-model:0.1",
                                    "env": [
                                        {
                                            "name": "FOO",
                                            "value": "bar"
                                        }
                                    ],
                                    "resources": {
                                        "requests": {
                                            "cpu": "500m",
                                            "memory": "100Mi"
                                        }
                                    }
                                }
                            ]
                        },
                        "hpaSpec": {
                            "minReplicas": 1,
                            "maxReplicas": 5,
                            "metrics": [
                                {
                                    "type": "Resource",
                                    "resource": {
                                        "name": "cpu",
                                        "targetAverageUtilization": 10
                                    }
                                }
                            ]
                        }
                    }
                ],
                "replicas": 1,
                "engineResources": {},
                "svcOrchSpec": {}
            }
        ]
    },
    "status": {}
}